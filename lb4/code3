import numpy as np

print("Знайти добуток матриць:")
m_sqr_arr = np.array([[5, 8, -4], [6, 9, -5], [4, 7, -3]])
print("Матриця 1")
print(m_sqr_arr)
m_sqr_arr = np.array([[3, 2, 5], [4, -1, 3], [9, 6, 5]])
print("Матриця 2")
print(m_sqr_arr)

a = np.matrix('5, 8, -4; 6, 9, -5; 4, 7, -3')
b = np.matrix('3, 2, 5; 4, -1, 3; 9, 6, 5')
c = a * b

print('Result a * b')
print(c)
